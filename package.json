{
  "name": "diary-community-server",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ]
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "test": "jest",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.18.1",
    "concurrently": "^3.5.0",
    "cookie-session": "^1.3.2",
    "express": "^4.15.5",
    "jest": "20.0.4",
    "jsonfile": "^4.0.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.13",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "rsvp": "^4.0.1",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "faker": "^4.1.0"
  }
}
